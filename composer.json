{
    "name": "jwage/tester",
    "type": "library",
    "description": "PHPUnit Tester",
    "keywords": ["tests", "phpunit"],
    "homepage": "http://github.com/jwage/tester",
    "license": "MIT",
    "authors": [
        {"name": "Jonathan H. Wage", "email": "jonwage@gmail.com"},
        {"name": "Kris Wallsmith", "email": "kris.wallsmith@gmail.com"}
